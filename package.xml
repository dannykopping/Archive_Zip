<!DOCTYPE package SYSTEM "package.dtd">
<package version="1.0">
  <name>Archive_Zip</name>
  <summary>Zip file management class</summary>
  <description>This class provides handling of zip files in PHP.
It supports creating, listing, extracting and adding to zip files.
</description>
  <license>LGPL</license>
  <maintainers>
    <maintainer>
      <user>vblavet</user>
      <role>lead</role>
      <name>Vincent Blavet</name>
      <email>vincent@blavet.net</email>
    </maintainer>
  </maintainers>
  <release>
    <version>0.1.0</version>
    <date>2005-11-20</date>
    <state>beta</state>
    <notes>First release
</notes>
    <provides type="class" name="Archive_Zip" />
    <provides type="function" name="Archive_Zip::create" />
    <provides type="function" name="Archive_Zip::add" />
    <provides type="function" name="Archive_Zip::delete" />
    <provides type="function" name="Archive_Zip::duplicate" />
    <provides type="function" name="Archive_Zip::listContent" />
    <provides type="function" name="Archive_Zip::merge" />
    <provides type="function" name="Archive_Zip::extract" />
    <provides type="function" name="Archive_Zip::properties" />
    <filelist>
      <file role="php" baseinstalldir="/" name="Archive/Zip.php"/>
    </filelist>
  </release>
  <changelog>
    <release>
      <version>0.1.0</version>
      <date>2005-11-20</date>
      <notes>First release</notes>
    </release>
  </changelog>
</package>
